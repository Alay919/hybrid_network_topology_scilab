clc; clear; close;

n=25;
edges=[
1 2;1 3;1 4;1 5;1 6;1 7;1 8;
9 10;10 11;11 12;12 13;13 14;
14 15;15 16;16 17;17 9;
18 19;19 20;20 21;21 22;22 23;23 24;24 25;
4 9;12 18
];

A=zeros(n,n);
for i=1:size(edges,1)
    A(edges(i,1),edges(i,2))=1;
    A(edges(i,2),edges(i,1))=1;
end

deg=sum(A,'r')';
disp("Edges connected to each node:");
disp(deg);

[maxEdges,node]=max(deg);
disp("Node with maximum edges:");
disp(node);
disp("Maximum edges:");
disp(maxEdges);
